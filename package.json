{
	"name": "fonzi2",
	"version": "1.4.2",
	"description": "Discord bot starter kit",
	"type": "commonjs",
	"engines": {
		"node": ">=18"
	},
	"packageManager": "pnpm@9.4.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "vitest",
		"build": "rimraf dist && tsup --config tsup.config.ts && pnpm copyfiles",
		"copyfiles": "copyfiles -u 1 views/**/* dist/views && copyfiles -u 1 public/**/* dist/public",
		"release": "pnpm build && changeset publish",
		"lint": "biome check ./src && tsc",
		"lint:fix": "biome check --write --unsafe ./src"
	},
	"keywords": ["discord", "bot", "typescript"],
	"author": "ljs360d",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.7.2",
		"cookie-session": "^2.1.0",
		"dotenv": "^16.4.5",
		"winston": "^3.13.0",
		"winston-transport": "^4.7.0"
	},
	"peerDependencies": {
		"discord.js": ">=14.14.1",
		"ejs": ">=3.1.9",
		"express": ">=4.18.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.2",
		"@changesets/cli": "^2.27.5",
		"@types/chai": "^4.3.16",
		"@types/cookie-session": "^2.0.49",
		"@types/express": "^4.17.21",
		"@types/node": "^20.14.7",
		"chai": "^5.1.1",
		"copyfiles": "^2.4.1",
		"discord.js": "^14.15.3",
		"ejs": "^3.1.10",
		"express": "^4.19.2",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^5.0.7",
		"tsup": "^8.1.0",
		"typescript": "^5.5.2",
		"vitest": "^1.6.0"
	},
	"changeset": {
		"defaultBranch": "master"
	},
	"publishConfig": {
		"access": "public"
	}
}
