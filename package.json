{
	"name": "fonzi2",
	"version": "1.0.1",
	"description": "Discord bot starter kit",
	"type": "commonjs",
	"engines": {
		"node": ">=18"
	},
	"packageManager": "pnpm@8.14.1",
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "vitest",
		"build": "rimraf dist && tsup --config tsup.config.ts && pnpm copyfiles",
		"copyfiles": "copyfiles -u 1 views/**/* dist/views && copyfiles -u 1 public/**/* dist/public",
		"release": "pnpm build && changeset publish",
		"lint": "prettier --write ./src/**/*.ts && tsc"
	},
	"keywords": [
		"discord",
		"bot",
		"typescript"
	],
	"author": "ljs360d",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.6.5",
		"cookie-session": "^2.0.0",
		"dotenv": "^16.3.1"
	},
	"peerDependencies": {
		"discord.js": ">=14.14.1",
		"ejs": ">=3.1.9",
		"express": ">=4.18.2"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@types/chai": "^4.3.11",
		"@types/cookie-session": "^2.0.48",
		"@types/express": "^4.17.21",
		"@types/node": "^20.11.0",
		"chai": "^5.0.0",
		"copyfiles": "^2.4.1",
		"discord.js": "^14.14.1",
		"ejs": "^3.1.9",
		"express": "^4.18.2",
		"prettier": "^3.2.1",
		"reflect-metadata": "^0.2.1",
		"rimraf": "^5.0.5",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^1.2.0"
	},
	"changeset": {
		"defaultBranch": "master"
	},
	"publishConfig": {
		"access": "public"
	}
}
